	<Button name="iWOWQueryQHDetailAcceptedLocationButton" parent="QuestHistoryDetailListScrollChildFrame" hidden="true">
		<Size>
			<AbsDimension x="196" y="12"/>
		</Size>
		<NormalText name="iWOWQueryQHDetailAcceptedLocationText" inherits="GameFontHighlightSmall" justifyH="LEFT">
			<Size>
				<AbsDimension x="196" y="12"/>
			</Size>
			<Anchors>
