        <Offset>
          <AbsDimension x="-30" y="10"/>
        </Offset>
      </Anchor>
    </Anchors>
    <Frames>
      <Button name="MerDKPMinimapButton">
        <HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
        <DisabledTexture file="Interface\Minimap\UI-Minimap-ZoomOutButton-Disabled"/>
        <Size>
          <AbsDimension x="33" y="33"/>
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="0" y="0"/>
            </Offset>
          </Anchor>
        </Anchors>
        <Layers>
          <Layer level="BORDER">
            <Texture name="$parentIcon" file="Interface\AddOns\IEDKP\mblogo">
              <Size>
                <AbsDimension x="20" y="20"/>
              </Size>
              <Anchors>
                <Anchor point="CENTER">
                  <Offset>
                    <AbsDimension x="-1" y="0"/>
                  </Offset>
                </Anchor>
              </Anchors>
            </Texture>
          </Layer>
          <Layer level="OVERLAY">
            <Texture name="$parentBorder" file="Interface\Minimap\MiniMap-TrackingBorder">
              <Size>
                <AbsDimension x="52" y="52"/>
              </Size>
              <Anchors>
                <Anchor point="TOPLEFT"/>
              </Anchors>
            </Texture>
          </Layer>
        </Layers>
        <Scripts>
          <OnLoad>
            getglobal(self:GetName().."Icon"):SetVertexColor(1, 1, 0);
            self.tooltipText = "IEDKP插件";
          </OnLoad>
          <OnClick>
            ShowUIPanel(CT_RaidTrackerFrame);
            MerDKPFrame:Show();
          </OnClick>
          <OnMouseDown>
            if ( button == "RightButton" ) then
            MerDKPMinimap:StartMoving();
            MerDKPMinimap.isMoving = true;
            MerDKPMinimap:SetScript("OnUpdate", snapMinimap);
            end
          </OnMouseDown>
          <OnMouseUp>
            if ( button == "RightButton" ) then
            MerDKPMinimap:StopMovingOrSizing();
            MerDKPMinimap.isMoving = false;
            MerDKPMinimap:SetScript("OnUpdate", nil);
            end
          </OnMouseUp>
          <OnEnter>
            if ( self.tooltipText ) then
            GameTooltip:SetOwner(self, "ANCHOR_LEFT");
            GameTooltip:SetText(self.tooltipText, nil, nil, nil, nil, 1);
            GameTooltip:Show();
            end
          </OnEnter>
          <OnLeave>
            GameTooltip:Hide();
          </OnLeave>
        </Scripts>
      </Button>
    </Frames>
    <Scripts>
      <OnLoad>
        self:RegisterEvent("VARIABLES_LOADED");
      </OnLoad>
    </Scripts>
  </Frame>
